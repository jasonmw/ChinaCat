@model Tuple<long, long[]>
@await Html.PartialAsync("_MyPartial", Model.Item1)
<div id="nums" hx-swap-oob="true">
    <ul class="list-group">
        @for(int i = 0; i < Model.Item2.Length; i++)
        {
            <li class="list-group-item">@Model.Item2[i].ToString("#,##0")</li>
        }
    </ul>
</div>
